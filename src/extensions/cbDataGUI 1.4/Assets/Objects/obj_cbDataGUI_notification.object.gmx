    case false:
        // Draw row bg
        draw_set_alpha(alpha);
        draw_set_color(global._cbColors[_cbDataGUIColor.notification]);
        draw_rectangle(_c.x + _c._bodyW - 8, _c.y + 5 , _c.x + _c._bodyW - 17 - string_width(message) , _c.y + 21, false);
        
